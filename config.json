{
  "output" : "../build/",
  "locals": {
    "url": "http://localhost:8080",
    "name": "Gitin' It",
    "description": "Understanding Distributed Version Control with Git"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./plugins/relative.coffee"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 10
  }
}
